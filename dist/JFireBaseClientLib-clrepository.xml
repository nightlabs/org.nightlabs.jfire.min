<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE classloader-repository
PUBLIC "-//NightLabs GmbH//DTD ClassLoader Repository 1.0//EN"
"http://www.nightlabs.de/dtd/clrepository_1_0.dtd">

<classloader-repository>
<!--
    The following element publishes all packages beginning with
    'com.nightlabs.' or 'org.nightlabs.' for the current directory
    (no target means the same as 'target="."') and recursively all
    directories that don't override.
-->
	<publish target="JFireBaseClientLib.jar">
		<resources>org\/nightlabs\/jfire\/base\/idgenerator\/.*</resources>
		<resources>org\/nightlabs\/jfire\/base\/jdo\/.*</resources>
	</publish>
</classloader-repository>
